doctype 5
html
  head
    meta(name="viewport", content="width=device-width initial-scale=1.0")
    title Tribute
    != nap.css('all', gzipSupport)

  body
    script(type='text/x-handlebars')
      h1
        | Welcome to Tribute!
      {{outlet}}

    script(type="text/x-handlebars", data-template-name="index")
      <ul>
      {{#each item in model}}
      <li>{{item}}</li>
      {{/each}}
      </ul>

    #scripts
      != nap.js('vendor', gzipSupport)
      != nap.js('client', gzipSupport)
