@js =
  vendor: [
    '/vendor/jquery/*.js'
    '/vendor/bootstrap/**/*.js'
    '/vendor/emberjs/handlebars*.js'
    '/vendor/emberjs/ember*.js'
  ]
  server: [
    '/app/server/**/*.coffee'
  ]
  client: [
    '/app/client/**/*.coffee'
  ]

@css =
  all: [
    '/vendor/**/*.css'
  ]

@jst =
  templates: [
    '/app/templates/**/*.jade'
  ]
